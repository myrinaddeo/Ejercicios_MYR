Proceso n_cadenas
	Definir num, i como entero;
	Definir frase como cadena;
	
	Escribir Sin Saltar "Escriba una frase.";
	Leer frase;
	
	
	Repetir
	Escribir Sin Saltar "Escriba la cantidad de repeticiones:";
	Leer num;
	Hasta que num>0
	
	Para i<-1 hasta num con paso 1 Hacer
		Escribir frase;
		
	FinPara
	
	
FinProceso
