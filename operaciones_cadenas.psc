Proceso operaciones_cadenas
	Definir frase, frase2, frase3 como cadena;
	Definir tamanio, tamanio2, num como entero;
	
	frase<- "Hola mundo!";
	frase2 <-subcadena(frase,0,3);
	tamanio<- Longitud(frase);
	tamanio2<- Longitud(frase2);
	
	frase3<-"110";
	num<- ConvertirANumero(frase3)+5;
	
	
	
	
	Escribir tamanio;
	Escribir tamanio2;
	Escribir frase3;
	Escribir num;
	
	
	
	
FinProceso